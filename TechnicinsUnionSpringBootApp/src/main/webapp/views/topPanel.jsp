<div id="commonTopPanel">
	<fieldset>
		<legend>
			<h4>Member Details</h4>
		</legend>
		<table border="0">
			<tr>
				<td style="width: 12%;">Name</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlMemberName' id='tpnl_memberName' class='texBoxCss'
					disabled="true"></td>
				<td style="width: 12%;">Card Balance</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlCardBalance' id='tpnl_cardBalance' class='texBoxCss'
					disabled="true"></td>

				<td rowspan="5" align="center" style="width: 15%;"><div id="tpnl_profilePicDiv"
						><img id="tpnl_profilePic" alt="" src=""></div></td>
			</tr>
			<tr>
				<td style="width: 12%;">Department</td>
				<td style="width: 30%;" align="center"><input type='hidden'
					name='tpnlDeptId' id='tpnl_deptId' class='texBoxCss'
					disabled="true">
					
					
					
					<input type='text'
					name='tpnlDeparmentName' id='tpnl_deparmentName' class='texBoxCss'
					disabled="true"></td>
				<td style="width: 12%;">Loan Balance</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlLoanBalance' id='tpnl_loanBalance' class='texBoxCss'
					disabled="true"></td>
			</tr>
			<tr>
				<td style="width: 12%;">Card No</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlCardNo' id='tpnl_cardNo' class='texBoxCss'
					disabled="true"></td>
				<td style="width: 12%;">Phone No</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlPhoneNo' id='tpnl_phoneNo' class='texBoxCss'
					disabled="true"></td>
			</tr>
			<tr>
				<td style="width: 12%;">Joining Date</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlDateOfJoining' id='tpnl_dateOfJoining' class='texBoxCss'
					disabled="true"></td>
				<td style="width: 12%;">Receipt No</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlReceiptNo' id='tpnl_receiptNo' class='texBoxCss'
					disabled="true"></td>
			</tr>
		 	<tr>
		 	<td style="width: 12%;">Father's Name</td>
				<td style="width: 30%;" align="center"><input type='text'
					name='tpnlFatherName' id='tpnl_fatherName' class='texBoxCss'
					disabled="true"></td>
					
				<td style="width: 12%;" >Address</td>
				<td style="width: 30%;"  align='center'>
				<textarea name='tpnlAddress' id='tpnl_address' style="border: 1px solid #ccc;    box-shadow: inset 0 1px 3px #ddd;    border-radius: 4px;    -webkit-box-sizing: border-box;    -moz-box-sizing: border-box;    box-sizing: border-box;    padding: 5px;width: 90% !important;" disabled="true"></textarea></td>
			</tr>	 
		</table>
	</fieldset>
</div>



